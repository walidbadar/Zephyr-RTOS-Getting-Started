/ {
	model = "Espressif ESP32-DEVKITC-WROOM-32D";
	compatible = "espressif,esp32";

	aliases {
		sw0 = &btn0;
	};

	buttons {
		compatible = "gpio-keys";
		btn0: btn_0 {
			gpios = <&gpio1 1 (GPIO_PULL_UP | GPIO_ACTIVE_HIGH)>;
			label = "User Button";
		};
	};
};

&wifi {
	status = "okay";
};
